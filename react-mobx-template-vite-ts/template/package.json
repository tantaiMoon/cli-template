    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^16.2.0",
    "husky": "^9.1.6",
    "lint-staged": "^16.1.0",
    "postcss": "^8.4.47",
    "postcss-preset-env": "^10.0.9",
    "prettier": "^3.3.3",
