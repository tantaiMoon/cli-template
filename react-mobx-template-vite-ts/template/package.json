{
  "name": "<%= data.name %>",
  "private": true,
  "version": "0.2.1",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production tsc -b && vite build",
    "lint": "eslint src/**/*.{ts,tsx} -c ./eslint.config.js",
    "format": "prettier --write",
    "lint:fix": "eslint -c ./eslint.config.js src/**/*.{ts,tsx} --fix --quiet",
    "preview": "vite preview",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "npm run lint:fix",
      "npm run format",
      "git add ."
    ],
    "*.{html,css,scss}": [
      "npm run format",
      "git add ."
    ]
  },
